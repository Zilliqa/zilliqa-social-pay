{
  "name": "zilliqa-social-pay",
  "version": "0.0.1",
  "scripts": {
    "db:migrate": "npx sequelize db:migrate",
    "db:seed:all": "npx sequelize db:seed:all",
    "db:create": "npx sequelize db:create",
    "dev": "npm run db:migrate && npm run db:seed:all && cross-env DEBUG=zilliqa-social-pay:* node ./server/index.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production npm run db:migrate && cross-env NODE_ENV=production npm run db:seed:all && cross-env NODE_ENV=production node ./server/index.js",
    "type-check": "tsc",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "precommit": "npm run lint:ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "@zilliqa-js/zilliqa": "^0.8.9",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "effector": "^20.12.1",
    "effector-react": "^20.6.2",
    "express": "^4.17.1",
    "isomorphic-unfetch": "3.0.0",
    "jsonwebtoken": "^8.5.1",
    "next": "latest",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.1",
    "passport-twitter-token": "^1.3.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-responsive-carousel": "^3.1.51",
    "react-spinners": "^0.8.1",
    "react-twitter-auth": "0.0.13",
    "react-twitter-embed": "^3.0.3",
    "sequelize": "^5.21.5",
    "styled-components": "^5.0.1",
    "twitter": "^1.7.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-loader": "^2.4.3",
    "@types/react-twitter-auth": "0.0.1",
    "@types/styled-components": "^5.0.1",
    "cross-env": "^7.0.2",
    "husky": "^4.2.3",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.3"
  },
  "license": "MIT"
}
