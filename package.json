{
  "name": "zilliqa-social-pay",
  "version": "0.0.3",
  "description": "SocialPay is an innovative new solution that allows you to earn $ZIL by sharing social media updates on Twitter.",
  "scripts": {
    "db:migrate": "npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "db:create": "npx sequelize db:create",
    "tx:handler": "node server/tx-handler | npx bunyan",
    "scheduler": "node server/scheduler | npx bunyan",
    "build": "next build",
    "start": "node ./server/index.js | npx bunyan",
    "type-check": "tsc",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "precommit": "npm run lint:ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "@zilliqa-js/zilliqa": "^0.11.1",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "effector": "^21.0.3",
    "effector-react": "^21.0.4",
    "express": "^4.17.1",
    "isomorphic-unfetch": "3.0.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "next": "latest",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.1",
    "passport-twitter-token": "^1.3.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-paginate": "^6.3.2",
    "react-responsive": "^8.1.0",
    "react-spinners": "^0.9.0",
    "react-twitter-auth": "0.0.13",
    "react-twitter-embed": "^3.0.3",
    "redis": "^3.0.2",
    "sequelize": "^5.21.8",
    "socket.io": "^2.3.0",
    "styled-components": "^5.1.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "twitter": "^1.7.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-loader": "^2.4.4",
    "@types/react-modal": "^3.10.6",
    "@types/react-paginate": "^6.2.1",
    "@types/react-responsive": "^8.0.2",
    "@types/react-transition-group": "^4.4.0",
    "@types/react-twitter-auth": "0.0.1",
    "@types/socket.io-client": "^1.4.33",
    "@types/styled-components": "^5.1.1",
    "@types/uuid": "^8.0.0",
    "husky": "^4.2.5",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "license": "MIT"
}
